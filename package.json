{
    "name": "ivy-mypyvy-syntax-highlighting",
    "publisher": "dranov",
    "repository": {
        "type": "git",
        "url": "https://github.com/dranov/ivy-mypyvy-syntax-highlighting.git"
    },
    "displayName": "Ivy & mypyvy Syntax Highlighting",
    "description": "Syntax highlighting for Ivy & mypyvy files",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.59.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "pyv",
                "aliases": [
                    "mypyvy",
                    "pyv"
                ],
                "extensions": [
                    "pyv"
                ],
                "configuration": "./mypyvy-language-configuration.json"
            },
            {
                "id": "ivy",
                "aliases": [
                    "ivy"
                ],
                "extensions": [
                    "ivy"
                ],
                "configuration": "./ivy-language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "pyv",
                "scopeName": "source.pyv",
                "path": "./syntaxes/pyv.tmLanguage.json"
            },
            {
                "language": "ivy",
                "scopeName": "source.ivy",
                "path": "./syntaxes/ivy.tmLanguage.json"
            }
        ]
    }
}